        <Label Content="Name :" HorizontalAlignment="Left" Margin="10,37,0,0" VerticalAlignment="Top" FontSize="16"/>
        <Label Content="Welcome to the online Concert Seat Booking. Click on the Seat which you want to Book" HorizontalAlignment="Left" VerticalAlignment="Top" Width="640" FontSize="16" Foreground="#FF861F1F"/>
        <Label Content="Click on the Reserved Seat if you want to remove it." HorizontalAlignment="Left" Margin="29,280,0,0" VerticalAlignment="Top" Width="298" Foreground="#FF0E20E7"/>
        <Label Content="Reserved" HorizontalAlignment="Left" Margin="85,310,0,0" VerticalAlignment="Top"/>
        <Label Content="Selected" HorizontalAlignment="Left" Margin="87,349,0,0" VerticalAlignment="Top"/>
        <Label Content="Available" HorizontalAlignment="Left" Margin="88,386,0,0" VerticalAlignment="Top"/>
        <Rectangle Fill="#FFF61E1E" HorizontalAlignment="Left" Height="25" Margin="10,311,0,0" VerticalAlignment="Top" Width="63"/>
