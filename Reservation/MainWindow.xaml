        <Button x:Name="btnload" Content="Load Data" HorizontalAlignment="Left" Margin="442,386,0,0" VerticalAlignment="Top" Width="75" FontSize="14" Click="btnload_Click" Background="#FFEE6D6D"/>

        <!--<Button x:Name="btnRemove" Content="Remove" HorizontalAlignment="Left" Margin="175,340,0,0" VerticalAlignment="Top" Width="75" Click="btnRemove_Click" />-->

    </Grid>
</Window>
