        xmlns:local="clr-namespace:Reservation"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="Reservation.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,-6.4,-2">
        <Rectangle Fill="#FF5094BD" HorizontalAlignment="Left" Height="129" Margin="6,90,0,0" VerticalAlignment="Top" Width="361"/>
