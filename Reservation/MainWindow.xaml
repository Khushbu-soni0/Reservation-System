        <Button x:Name="btn4" Content="4" HorizontalAlignment="Left" Margin="269,107,0,0" VerticalAlignment="Top" Width="75" Click="btn4_Click"/>
        <Button x:Name="btn5" Content="5" HorizontalAlignment="Left" Margin="29,131,0,0" VerticalAlignment="Top" Width="75" Click="btn5_Click"/>
        <Button x:Name="btn6" Content="6" HorizontalAlignment="Left" Margin="109,131,0,0" VerticalAlignment="Top" Width="75" Click="btn6_Click"/>
        <Button x:Name="btn7" Content="7" HorizontalAlignment="Left" Margin="189,131,0,0" VerticalAlignment="Top" Width="75" Click="btn7_Click"/>
        <Button x:Name="btn8" Content="8" HorizontalAlignment="Left" Margin="269,131,0,0" VerticalAlignment="Top" Width="75" Click="btn8_Click"/>
        <Button x:Name="btn9" Content="9" HorizontalAlignment="Left" Margin="29,155,0,0" VerticalAlignment="Top" Width="75" Click="btn9_Click"/>
        <Button x:Name="btn10" Content="10" HorizontalAlignment="Left" Margin="109,155,0,0" VerticalAlignment="Top" Width="75" Click="btn10_Click"/>
        <Button x:Name="btn11" Content="11" HorizontalAlignment="Left" Margin="189,155,0,0" VerticalAlignment="Top" Width="75" Click="btn11_Click"/>
        <Button x:Name="btn12" Content="12" HorizontalAlignment="Left" Margin="269,155,0,0" VerticalAlignment="Top" Width="75" Click="btn12_Click"/>
        <Button x:Name="btn13" Content="13" HorizontalAlignment="Left" Margin="29,179,0,0" VerticalAlignment="Top" Width="75" Click="btn13_Click"/>
        <Button x:Name="btn14" Content="14" HorizontalAlignment="Left" Margin="109,179,0,0" VerticalAlignment="Top" Width="75" Click="btn14_Click"/>
